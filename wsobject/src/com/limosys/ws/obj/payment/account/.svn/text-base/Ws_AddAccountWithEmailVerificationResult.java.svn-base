package com.limosys.ws.obj.payment.account;

import com.limosys.ws.obj.Ws_Base;

public class Ws_AddAccountWithEmailVerificationResult extends Ws_Base {
	private boolean accountAdded = false;
	private boolean verificationEmailSent = false;
	private boolean accountAlreadyAdded = false;	
	public boolean isAccountAdded() {
		return accountAdded;
	}
	public void setAccountAdded(boolean accountAdded) {
		this.accountAdded = accountAdded;
	}
	public boolean isVerificationEmailSent() {
		return verificationEmailSent;
	}
	public void setVerificationEmailSent(boolean verificationEmailSent) {
		this.verificationEmailSent = verificationEmailSent;
	}
	public boolean isAccountAlreadyAdded() {
		return accountAlreadyAdded;
	}
	public void setAccountAlreadyAdded(boolean accountAlreadyAdded) {
		this.accountAlreadyAdded = accountAlreadyAdded;
	}	
}
