package com.limosys.ws.obj.param;

public class Ws_DeviceRegistration {
	private int custId;
	private String deviceId; // From client, not phone number
	
	private String error;
  private String deviceType;
  private String registrationToken;
  private String verification;

  public Ws_DeviceRegistration() {
  }

  public Ws_DeviceRegistration(String deviceId, String deviceType, String registrationToken) {
      this.deviceId = deviceId;
      this.deviceType = deviceType;
      this.registrationToken = registrationToken;
  }

  public int getCustId() {
		return custId;
	}

	public void setCustId(int custId) {
		this.custId = custId;
	}
  
  public String getError() {
      return error;
  }

  public void setError(String error) {
      this.error = error;
  }

  public String getDeviceId() {
      return deviceId;
  }

  public void setDeviceId(String deviceId) {
      this.deviceId = deviceId;
  }

  public String getDeviceType() {
      return deviceType;
  }

  public void setDeviceType(String deviceType) {
      this.deviceType = deviceType;
  }

  public String getRegistrationToken() {
      return registrationToken;
  }

  public void setRegistrationToken(String registrationToken) {
      this.registrationToken = registrationToken;
  }

	public String getVerification() {
		return verification;
	}

	public void setVerification(String verification) {
		this.verification = verification;
	}
}
