package com.limosys.ws.obj.param;

import com.limosys.ws.obj.profile.Ws_Profile;

public class Ws_UsernamePasswordParam {
	private Ws_Profile profile;
	private String username;
	private String password;
	private String deviceId;
	
	private String prevPassword;

	public Ws_UsernamePasswordParam(Ws_Profile profile, String username, String password, String prevPassword, String deviceId) {
		this.setProfile(profile);
		this.setUsername(username);
		this.setPassword(password);
		this.setPrevPassword(prevPassword);
		this.setDeviceId(deviceId);
	}

	public Ws_Profile getProfile() {
		return profile;
	}

	public void setProfile(Ws_Profile profile) {
		this.profile = profile;
	}

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getDeviceId() {
		return deviceId;
	}

	public void setDeviceId(String deviceId) {
		this.deviceId = deviceId;
	}

	public String getPrevPassword() {
		return prevPassword;
	}

	public void setPrevPassword(String prevPassword) {
		this.prevPassword = prevPassword;
	}
	
	

}
