package com.limosys.ws.obj.param;

public class Ws_GetTerminalByGpsParam {
	private String airportCd;
	private double latitude;
	private double longitude;
	
	public Ws_GetTerminalByGpsParam(String airportCd, double latitude, double longitude) {
		this.setAirportCd(airportCd);
		this.setLatitude(latitude);
		this.setLongitude(longitude);
	}

	public String getAirportCd() {
		return airportCd;
	}

	public void setAirportCd(String airportCd) {
		this.airportCd = airportCd;
	}

	public double getLatitude() {
		return latitude;
	}

	public void setLatitude(double latitude) {
		this.latitude = latitude;
	}

	public double getLongitude() {
		return longitude;
	}

	public void setLongitude(double longitude) {
		this.longitude = longitude;
	}
}
