package com.limosys.ws.obj.param;

import com.limosys.ws.obj.job.Ws_JobObj;

public class Ws_CarPricesParam {

	private boolean requestAffCarPrice = false;
	private Ws_JobObj jobObj;
	private String senderCompId;
	private String senderSysComp;
	private String destCompId;

	public Ws_CarPricesParam(Ws_JobObj jobObj) {
		this.jobObj = jobObj;
	}

	public boolean isRequestAffCarPrice() {
		return requestAffCarPrice;
	}

	public void setRequestAffCarPrice(boolean requestAffCarPrice) {
		this.requestAffCarPrice = requestAffCarPrice;
	}

	public Ws_JobObj getJobObj() {
		return jobObj;
	}

	public void setJobObj(Ws_JobObj jobObj) {
		this.jobObj = jobObj;
	}

	public String getSenderCompId() {
		return senderCompId;
	}

	public void setSenderCompId(String senderCompId) {
		this.senderCompId = senderCompId;
	}

	public String getSenderSysComp() {
		return senderSysComp;
	}

	public void setSenderSysComp(String senderSysComp) {
		this.senderSysComp = senderSysComp;
	}

	public String getDestCompId() {
		return destCompId;
	}

	public void setDestCompId(String destCompId) {
		this.destCompId = destCompId;
	}
}
