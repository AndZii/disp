package com.limosys.ws.obj.profile;

import com.limosys.ws.obj.Ws_Base;

public class Ws_Profile extends Ws_Base {

	private int custId;
	private String username;
	private String nameLast;
	private String nameFirst;

	private boolean active = false;

	private String phoneNumber;
	private boolean phoneNumberActive;

	private String email;
	private Boolean emailVerified = null;

	private int activeJob = 0;

	private String compId = "";

	private double defaultGratuityPct = 0.0d;

	private boolean isChargeAgree = false;
	
	private String facebookUID;
	private String googleUID;
	
	private String alephRegistrationMsg;
	private boolean hasAlephProfile;
	private boolean acctEmployee = false;
	private String acctId;
	private boolean phoneNumIsAlreadyActivated = false;

	public Ws_Profile() {
		custId = -1;
	}

	public Ws_Profile(String nameFirst, String nameLast) {
		this.setNameFirst(nameFirst);
		this.setNameLast(nameLast);
		this.setUsername("");
		this.setPhoneNumber("");
		this.setEmail("");
	}

	public Ws_Profile(int custId, String username, String email, String number, String nameFirst, String nameLast) {
		this.setCustId(custId);
		this.setUsername(username);
		this.setNameFirst(nameFirst);
		this.setNameLast(nameLast);
		this.setPhoneNumber(number);
		this.setEmail(email);
	}

	public Ws_Profile(int custId) {
		this.setCustId(custId);
	}

	public int getCustId() {
		return custId;
	}

	public void setCustId(int custId) {
		this.custId = custId;
	}

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getNameLast() {
		return nameLast;
	}

	public void setNameLast(String nameLast) {
		this.nameLast = nameLast;
	}

	public String getNameFirst() {
		return nameFirst;
	}

	public void setNameFirst(String nameFirst) {
		this.nameFirst = nameFirst;
	}

	public String getPhoneNumber() {
		return phoneNumber;
	}

	public void setPhoneNumber(String phoneNumber) {
		this.phoneNumber = phoneNumber;
	}

	public boolean isPhoneNumberActive() {
		return phoneNumberActive;
	}

	public void setPhoneNumberActive(boolean phoneNumberActive) {
		this.phoneNumberActive = phoneNumberActive;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public int getActiveJob() {
		return activeJob;
	}

	public void setActiveJob(int activeJob) {
		this.activeJob = activeJob;
	}

	public String getCompId() {
		return compId;
	}

	public void setCompId(String compId) {
		this.compId = compId;
	}

	public boolean isChargeAgree() {
		return isChargeAgree;
	}

	public void setChargeAgree(boolean isChargeAgree) {
		this.isChargeAgree = isChargeAgree;
	}

	public double getDefaultGratuityPct() {
		return defaultGratuityPct;
	}

	public void setDefaultGratuityPct(double defaultGratuityPct) {
		this.defaultGratuityPct = defaultGratuityPct;
	}

	public String getFacebookUID() {
		return facebookUID;
	}

	public void setFacebookUID(String facebookUID) {
		this.facebookUID = facebookUID;
	}

	public String getAlephRegistrationMsg() {
		return alephRegistrationMsg;
	}

	public void setAlephRegistrationMsg(String alephRegistrationMsg) {
		this.alephRegistrationMsg = alephRegistrationMsg;
	}

	public boolean isHasAlephProfile() {
		return hasAlephProfile;
	}

	public void setHasAlephProfile(boolean hasAlephProfile) {
		this.hasAlephProfile = hasAlephProfile;
	}

	public boolean isAcctEmployee() {
	  return acctEmployee;
  }

	public void setAcctEmployee(boolean acctEmployee) {
	  this.acctEmployee = acctEmployee;
  }

	public String getGoogleUID() {
		return googleUID;
	}

	public void setGoogleUID(String googleUID) {
		this.googleUID = googleUID;
	}
	
	public boolean isPhoneNumIsAlreadyActivated() {
		return phoneNumIsAlreadyActivated;
	}

	public void setPhoneNumIsAlreadyActivated(boolean phoneNumIsAlreadyActivated) {
		this.phoneNumIsAlreadyActivated = phoneNumIsAlreadyActivated;
	}

	public String getAcctId() {
		return acctId;
	}

	public void setAcctId(String acctId) {
		this.acctId = acctId;
	}

	public boolean isActive() {
		return active;
	}

	public void setActive(boolean active) {
		this.active = active;
	}

	public Boolean getEmailVerified() {
		return emailVerified;
	}

	public void setEmailVerified(Boolean emailVerified) {
		this.emailVerified = emailVerified;
	}

}
