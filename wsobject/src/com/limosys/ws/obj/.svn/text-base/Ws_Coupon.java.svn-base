package com.limosys.ws.obj;

public class Ws_Coupon extends Ws_Base {

	private String couponCd;
	private String couponVerification;
	private String couponDesc;
	private double couponAmt;

	public Ws_Coupon() {
		super();
	}

	public Ws_Coupon(String couponCd) {
		this();
		this.couponCd = couponCd;
	}

	public Ws_Coupon(String code, double amt, String verification) {
		this(code);
		this.couponAmt = amt;
		this.couponVerification = verification;
	}

	public String getCouponCd() {
		return couponCd;
	}

	public void setCouponCd(String couponCd) {
		this.couponCd = couponCd;
	}

	public String getCouponVerification() {
		return couponVerification;
	}

	public void setCouponVerification(String couponVerification) {
		this.couponVerification = couponVerification;
	}

	public String getCouponDesc() {
		return couponDesc;
	}

	public void setCouponDesc(String couponDesc) {
		this.couponDesc = couponDesc;
	}

	public double getCouponAmt() {
		return couponAmt;
	}

	public void setCouponAmt(double couponAmt) {
		this.couponAmt = couponAmt;
	}
}
